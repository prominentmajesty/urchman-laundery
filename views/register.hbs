        <div class="col-md-8 mb-1.9 pt-4">
            <div class="form-group">
                <div class="form-check">
                    <input class="form-check-input" name="form-check-input" type="checkbox" value="" id="checkBox">
                    <label class="form-check-label" for="checkBox">
                        Agree to terms and conditions
                    </label>
                    <div class="valid-feedback" id="chk">
                        You must agree before submitting.
                    </div>
                </div>
            </div>
            <button class="btn btn-primary" id="submitbtn">Submit Request</button>
        </div>
    </form>
</div>
