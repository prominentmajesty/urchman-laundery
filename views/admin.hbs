{{> header}}
<div class = "holder">
    <div class="container container-admin">
        {{#if returnDoc}}
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">    
                <table class="table table-responsive table-hover">
                        <thead class="thead-light">
                            <tr>
                                <th scope="col">Index</th>
                                <th scope="col">Username</th>
                                <th scope="col">Contact</th>
                                <th scope="col">Email</th>
                                <th scope="col">Services</th>
                                <th scope="col">Delivery</th>
                                <th scope="col">Address</th>
                                <th scope="col">Items</th>
                                <th scope="col">Card Number</th>
                                <th scope="col">Card Expiring Date</th>
                                <th scope="col">Card Name</th>
                                <th scope="col">Card Code</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each returnDoc}}
                                <tr>
                                    <th scope="row">{{this.order}}</th>
                                    <td>{{this.UserName}}</td>
                                    <td>{{this.PhoneNumber}}</td>
                                    <td>{{this.EmailAdress}}</td>
                                    <td>{{this.SelectServices}}</td>
                                    <td>{{this.DeliveryType}}</td>
                                    <td>{{this.Address}}</td>
                                    <td>{{this.Items}}</td>
                                    <td>{{this.CardNumber}}</td>
                                    <td>{{this.ExpiringDate}}</td>
                                    <td>{{this.CardName}}</td>
                                    <td>{{this.CardCode}}</td>
                                    <td><button type="button" class="btn btn-danger">Delete</button><td></td>
                                </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        {{/if}}
    </div>
</div>
{{> footer}}