{{> header}}
<div class = "holder">
    <div class="container container-admin">
        {{#if returnDoc}}
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12" id = "getTable">  
                    <table class="table table-responsive table-hover">
                        <thead class="thead">
                            <tr>
                                <th scope="col">Index</th>
                                <th scope="col">Username</th>
                                <th scope="col">Contact</th>
                                <th scope="col">Email</th>
                                <th scope="col">Services</th>
                                <th scope="col">Delivery</th>
                                <th scope="col">Address</th>
                                <th scope="col">Items</th>
                                <th scope="col">Card Number</th>
                                <th scope="col">Card Expiring Date</th>
                                <th scope="col">Card Name</th>
                                <th scope="col">Card Code</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each returnDoc}}
                                <tr>
                                    <th scope="row">{{this.order}}</th>
                                    <td class = "id">{{this.UserName}}</td>
                                    <td class="id">{{this.PhoneNumber}}</td>
                                    <td class="email">{{this.EmailAdress}}</td>
                                    <td class="id">{{this.SelectServices}}</td>
                                    <td class="id">{{this.DeliveryType}}</td>
                                    <td class="id">{{this.Address}}</td>
                                    <td class="id">{{this.Items}}</td>
                                    <td class="id">{{this.CardNumber}}</td>
                                    <td class="id">{{this.ExpiringDate}}</td>
                                    <td class="id">{{this.CardName}}</td c>
                                    <td class="id">{{this.CardCode}}</td>
                                    <td><button type="button" id = "button" name="buttond" class="btn btn-danger">Delete</button></td>
                                </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        {{/if}}
    </div>
</div>
{{> footer}}